#!/bin/ksh
if [[ x"$1" == x"--dev" ]] ; then
	. s.ssmuse.dot $(cat ssmuse_dependencies-dev.bndl)
fi

if [[ -f .setenv_${COMP_ARCH}.dot ]] ; then
   echo ". .setenv_${COMP_ARCH}.dot"
   . .setenv_${COMP_ARCH}.dot
fi
if [[ -f ssmusedep_${COMP_ARCH}.bndl ]] ; then
   echo ". s.ssmuse.dot ssmusedep_${COMP_ARCH}.bndl"
   . s.ssmuse.dot $(cat ssmusedep_${COMP_ARCH}.bndl)
fi
if [[ -f ssmusedep.bndl ]] ; then
   echo ". s.ssmuse.dot ssmusedep.bndl"
   . s.ssmuse.dot $(cat ssmusedep.bndl)
fi


. $(pwd)/bin/.env_setup.dot rpnpy dev all $(pwd) $(pwd)

#export EC_LD_LIBRARY_PATH="$(true_path ~armnlib/LIBSO) $EC_LD_LIBRARY_PATH"
export PATH=$(pwd)/bin:$PATH
export PYVERSION="$(python -V 2>&1 | cut -d' ' -f2 | cut -d'.' -f1-2)"
export PYTHONPATH=$(pwd):$(pwd)/build/lib.$(uname -s | tr 'A-Z' 'a-z')-$(uname -m)-${PYVERSION}:$(pwd)/lib:${RMN_LIBPATH}:$PYTHONPATH
